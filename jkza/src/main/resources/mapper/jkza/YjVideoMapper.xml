<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.jkza.mapper.YjVideoMapper">

    <resultMap type="YjVideo" id="YjVideoResult">
        <result property="id"    column="id"    />
        <result property="buildingSiteId"    column="building_site_id"    />
        <result property="videoName"    column="video_name"    />
        <result property="lowPointName"    column="low_point_name"    />
        <result property="equipmentType"    column="equipment_type"    />
        <result property="onlineType"    column="online_type"    />
        <result property="coordinate"    column="coordinate"    />
        <result property="dataType"    column="data_type"    />
    </resultMap>

    <sql id="selectYjVideoVo">
        select id, building_site_id, video_name, equipment_type, online_type,coordinate, data_type from yj_video
    </sql>
    <sql id="selectYjVideoListAndName">
        select t1.id, t1.building_site_id, t1.video_name,t1.coordinate, t1.equipment_type, t1.online_type, t1.data_type ,t2.id,t2.low_point_name
        from yj_video t1
                 left join yj_low_point t2
                           on t1.building_site_id = t2.id
    </sql>

    <select id="selectYjVideoList" parameterType="YjVideo" resultMap="YjVideoResult">
        <include refid="selectYjVideoVo"/>
        <where>
            <if test="buildingSiteId != null "> and building_site_id = #{buildingSiteId}</if>
            <if test="videoName != null  and videoName != ''"> and video_name like concat('%', #{videoName}, '%')</if>
            <if test="equipmentType != null "> and equipment_type = #{equipmentType}</if>
            <if test="onlineType != null "> and online_type = #{onlineType}</if>
            <if test="coordinate != null  and coordinate != ''"> and coordinate = #{coordinate}</if>
            <if test="dataType != null "> and data_type = #{dataType}</if>
        </where>
    </select>
    <select id="selectYjVideoListAndName" parameterType="YjVideo" resultMap="YjVideoResult">
        <include refid="selectYjVideoListAndName"/>
        <where>
            <if test="buildingSiteId != null "> and t1.building_site_id = #{buildingSiteId}</if>
            <if test="videoName != null  and videoName != ''"> and t1.video_name like concat('%', #{videoName}, '%')</if>
            <if test="coordinate != null  and coordinate != ''"> and t1.coordinate = #{coordinate}</if>
            <if test="lowPointName != null  and lowPointName != ''"> and t2.low_point_name like concat('%', #{lowPointName}, '%')</if>
            <if test="equipmentType != null "> and t1.equipment_type = #{equipmentType}</if>
            <if test="onlineType != null "> and t1.online_type = #{onlineType}</if>
            <if test="dataType != null "> and t1.data_type = #{dataType}</if>
        </where>
    </select>

    <select id="selectYjVideoById" parameterType="Long" resultMap="YjVideoResult">
        <include refid="selectYjVideoVo"/>
        where id = #{id}
    </select>

    <insert id="insertYjVideo" parameterType="YjVideo" useGeneratedKeys="true" keyProperty="id">
        insert into yj_video
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="buildingSiteId != null">building_site_id,</if>
            <if test="videoName != null">video_name,</if>
            <if test="equipmentType != null">equipment_type,</if>
            <if test="onlineType != null">online_type,</if>
            <if test="coordinate != null">coordinate,</if>
            <if test="dataType != null">data_type,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="buildingSiteId != null">#{buildingSiteId},</if>
            <if test="videoName != null">#{videoName},</if>
            <if test="equipmentType != null">#{equipmentType},</if>
            <if test="onlineType != null">#{onlineType},</if>
            <if test="coordinate != null">#{coordinate},</if>
            <if test="dataType != null">#{dataType},</if>
        </trim>
    </insert>

    <update id="updateYjVideo" parameterType="YjVideo">
        update yj_video
        <trim prefix="SET" suffixOverrides=",">
            <if test="buildingSiteId != null">building_site_id = #{buildingSiteId},</if>
            <if test="videoName != null">video_name = #{videoName},</if>
            <if test="equipmentType != null">equipment_type = #{equipmentType},</if>
            <if test="onlineType != null">online_type = #{onlineType},</if>
            <if test="coordinate != null">coordinate = #{coordinate},</if>
            <if test="dataType != null">data_type = #{dataType},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteYjVideoById" parameterType="Long">
        delete from yj_video where id = #{id}
    </delete>

    <delete id="deleteYjVideoByIds" parameterType="String">
        delete from yj_video where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>